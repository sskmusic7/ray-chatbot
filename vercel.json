{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/test.txt",
      "dest": "/test.txt"
    },
    {
      "src": "/1.png",
      "dest": "/1.png"
    },
    {
      "src": "/supparay-logo.jpg",
      "dest": "/supparay-logo.jpg"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
